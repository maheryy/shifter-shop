{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "test", "lint"]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"]
    },
    "test": {
      "dependsOn": ["^test"]
    },
    "lint": {
      "dependsOn": []
    },
    "dev": {
      "dependsOn": ["^build"]
    },
    "start": {
      "dependsOn": ["build"]
    }
  },
  "affected": {
    "defaultBase": "main"
  }
}
